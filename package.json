{
  "name": "@ikenxuan/xhshow-ts",
  "version": "1.0.2",
  "description": "XHS API signature generator",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenxuan/xhshow-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsx watch src/dev.ts",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --access public"
  },
  "keywords": [
    "xhs",
    "signature",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.10.1",
    "axios": "npm:@karinjs/axios@^1.13.2",
    "tsdown": "^0.21.0-beta.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}